<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/entryFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.servicetesttool.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first" />
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.servicetesttool.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second" />
    <fragment
        android:id="@+id/entryFragment"
        android:name="com.example.servicetesttool.EntryFragment"
        android:label="fragment_entry"
        tools:layout="@layout/fragment_entry">
        <action
            android:id="@+id/action_entryFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
</navigation>